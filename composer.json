{
    "name": "veendhq/woocommerce",
    "description": "Vida by VeendHQ",
    "type": "wordpress-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Veend\\Woocommerce\\": "inc/"
        }
    },
    "authors": [
        {
            "name": "VeendHq Devs"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "makepot-audit": [
            "wp --allow-root i18n make-pot . --exclude=\".github,.wordpress-org,vendor,bin,node_modules,tests\" --slug=vidaveend --debug"
        ],
        "makepot": [
            "@makepot-audit --skip-audit"
        ]
    },
    "extra": {
        "scripts-description": {
            "makepot-audit": "Generate i18n/languages/vidaveend.pot file and run audit",
            "makepot": "Generate i18n/languages/vidaveend.pot file"
        }
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.0",
        "woocommerce/woocommerce-sniffs": "^1.0"
    }
}